{
    "name": "ldes-solid-server",
    "version": "0.0.1",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "engines": {
        "node": "^16.13.0"
    },
    "files": [
        "bin",
        "dist",
        "config"
